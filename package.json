{
  "name": "BTC2-Wallet",
  "version": "0.0.2",
  "description": "BTC2 wallet",
  "author": {
    "name": "Kzwo"
  },
  "keywords": [
    "angular",
    "angular 15",
    "electron",
    "electron 22",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve backend:serve",
    "ng:serve": "nx serve",
    "build": "npm run electron:serve-tsc && nx build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "web:build": "npm run build -- -c web-production",
    "backend:serve": "cd backend && nest start --watch",
    "electron": "electron",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 tcp:3000 && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "test": "nx test --watch=false",
    "test:watch": "nx test",
    "e2e": "npm run build:prod && playwright test -c e2e/playwright.config.ts e2e/",
    "e2e:show-trace": "playwright show-trace e2e/tracing/trace.zip",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "nx lint"
  },
  "dependencies": {
    "@angular/common": "15.1.5",
    "@angular/compiler": "15.1.5",
    "@angular/core": "15.1.5",
    "@angular/forms": "15.1.5",
    "@angular/language-service": "15.1.5",
    "@angular/platform-browser": "15.1.5",
    "@angular/platform-browser-dynamic": "15.1.5",
    "@angular/router": "15.1.5",
    "@nestjs/jwt": "10.0.3",
    "@nestjs/passport": "9.0.3",
    "bcrypt": "5.1.0",
    "jsonwebtoken": "9.0.0",
    "passport": "0.6.0",
    "passport-local": "1.0.0",
    "password-validator": "5.3.0",
    "rxjs": "7.8.0",
    "tailwindcss": "3.3.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "15.0.0",
    "@angular-devkit/build-angular": "15.1.6",
    "@angular-devkit/core": "15.1.6",
    "@angular-devkit/schematics": "15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.1.6",
    "@angular/compiler-cli": "15.1.5",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@nrwl/angular": "15.9.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.9.2",
    "@playwright/test": "1.30.0",
    "@schematics/angular": "15.1.6",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/jwt-decode": "3.1.0",
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.48.0",
    "@typescript-eslint/parser": "5.48.0",
    "conventional-changelog-cli": "2.2.2",
    "electron": "23.1.0",
    "electron-builder": "23.6.0",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.34.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.7.5",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.3.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "node-polyfill-webpack-plugin": "2.0.1",
    "npm-run-all": "4.1.5",
    "nx": "15.9.2",
    "playwright": "1.30.0",
    "ts-node": "10.9.1",
    "typescript": "~4.8.4",
    "wait-on": "7.0.1",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">= 14.20.0 || >= 16.13.0 || >= 18.10.0"
  },
  "browserslist": [
    "chrome 110"
  ]
}
